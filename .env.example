# RE Engine Environment Configuration
# Copy this file to .env and update with your actual values

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
# Database type: 'csv' (development), 'postgresql' (production), or 'supabase' (cloud)
DB_TYPE=csv

# CSV Configuration (Development)
DATA_DIR=./data

# PostgreSQL Configuration (Production)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=reengine
# DB_USERNAME=postgres
# DB_PASSWORD=your_password
# DB_SSL=false

# Database Configuration
DB_TYPE=csv # options: csv, postgresql, supabase
DATABASE_URL=postgres://user:password@host:port/dbname?sslmode=require
DATABASE_POOLED_URL=postgres://user:password-pooler@host:port/dbname?sslmode=require
DATABASE_POOL_SIZE=20
DATABASE_TIMEOUT=30000

